        android:id="@+id/btn1"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="270dp"
        android:text="EXISTING SURVEY"
        android:id="@+id/btn2"
        />


</RelativeLayout>